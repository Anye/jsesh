  <shortcut
     name="JSesh"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-Xmx256m -jar &quot;$INSTALL_PATH\libs\jsesh.jar&quot;"
     description="JSesh hieroglyphic editor"
	 iconFile="$INSTALL_PATH\icons\tksesh.ico"
     >
     <createForPack name="Base"/>
    </shortcut>

  <shortcut
     name="Sign Info Editor"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
   	 target="$INSTALL_PATH\bin\signInfoEditor.bat"
     description="Editor for documenting hieroglyphic signs"
     >
     <createForPack name="Base"/>
    </shortcut>
       
    <shortcut
     name="JSesh_Documentation"
     programGroup="yes"
     desktop="yes"
     startMenu="yes"
     startup="no"
     target="$INSTALL_PATH/jseshDoc.pdf"
	 description="JSesh editor documentation"
     >
   </shortcut>
</shortcuts>
