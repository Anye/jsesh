<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
    <SkipIfNotSupported/>
    <programGroup defaultName="JSesh" location="applications"/>
  
    <shortcut
     name="JSesh"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-Xmx256m -jar &quot;$INSTALL_PATH\lib\${project.build.finalName}.jar&quot;"
     description="JSesh hieroglyphic editor"
     iconFile="$INSTALL_PATH\icons\tksesh.ico"
     >
        <createForPack name="Base"/>
    </shortcut>

    <shortcut
     name="Sign Info Editor"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-Xmx256m -cp &quot;$INSTALL_PATH\lib\${project.build.finalName}.jar&quot; jsesh.signInfoEditor.ui.SignInfoEditorApplication"
     description="Editor for documenting hieroglyphic signs"
     >
        <createForPack name="Base"/>
    </shortcut>   
</shortcuts>
