<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="Jsesh" location="applications"/>


  <shortcut
     name="JSesh hieroglyphic editor"
     programGroup="yes"
     desktop="yes"
     startMenu="yes"
     startup="no"
     target="java"
     commandLine="-Xmx256m -jar &quot;$INSTALL_PATH/lib/jseshAppli-${project.version}.jar&quot;"
     type="Application"
	 description="JSesh hieroglyphic editor"
	 iconFile="$INSTALL_PATH/icons/hibou.png"
	 encoding="UTF-8"
     > 
     <createForPack name="Base"/>
   </shortcut>
     <shortcut
     name="Sign Info Editor"
     programGroup="yes"
     desktop="yes"
     startMenu="yes"
     startup="no"
     target="java"
     commandLine="-Xmx256m -cp &quot;$INSTALL_PATH/lib/jsesh-${project.version}.jar:$INSTALL_PATH/lib/jseshGlyphs-${project.version}.jar&quot;:jsesh.utilitySoftwares.signInfoEditor.ui.SignInfoEditorApplication"
     description="Editor for documenting hieroglyphic signs"
     type="Application"
     encoding="UTF-8"
     >
        <createForPack name="Base"/>
    </shortcut>     
</shortcuts>
